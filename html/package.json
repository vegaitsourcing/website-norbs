{
	"name": "fe-hbs-starter",
	"version": "3.2.2",
	"description": "Front-end HBS Starter",
	"author": "Front-end",
	"scripts": {
		"assets": "node ./node_scripts/tasks/assets",
		"build-dev": "node ./node_scripts/tasks/build-dev",
		"build-prod": "node ./node_scripts/tasks/build-prod",
		"build-stage": "node ./node_scripts/tasks/build-stage",
		"cf": "node ./node_scripts/tasks/createFile",
		"css": "node ./node_scripts/tasks/css",
		"hbs": "node ./node_scripts/tasks/hbs",
		"iconfont": "node ./node_scripts/tasks/iconfont",
		"js": "node ./node_scripts/tasks/js",
		"reset-dev": "node ./node_scripts/tasks/reset-dev",
		"watch": "node ./node_scripts/tasks/watch",
		"add-lng": "node ./node_scripts/tasks/add-lng",
		"config-eslint": "eslint ./**/*.js --ignore-pattern dist --fix",
		"eslint": "eslint ./src/**/*.js",
		"stylelint": "node ./node_scripts/tasks/css-lint",
		"server": "cd ../wwwroot && reload -b -p 3000"
	},
	"browserslist": [
		"last 2 versions",
		"ie >= 11",
		"ios >= 8"
	],
	"dependencies": {
		"jquery": "3.6.1",
		"scroll-behavior-polyfill": "^2.0.13",
		"swiper": "8.3.2"
	},
	"devDependencies": {
		"@babel/core": "7.19.0",
		"@babel/eslint-parser": "^7.18.9",
		"@babel/plugin-transform-for-of": "7.18.8",
		"@babel/preset-env": "7.19.0",
		"@babel/register": "7.18.9",
		"autoprefixer": "10.4.8",
		"babel-loader": "8.2.5",
		"chokidar": "3.5.3",
		"colors": "1.4.0",
		"core-js": "3.25.0",
		"eslint": "8.23.0",
		"eslint-webpack-plugin": "^3.2.0",
		"expose-loader": "4.0.0",
		"find-package-json": "1.2.0",
		"fs-extra": "10.1.0",
		"handlebars": "4.7.7",
		"js-beautify": "1.14.6",
		"postcss": "8.4.16",
		"postcss-flexbugs-fixes": "5.0.2",
		"reload": "^3.2.0",
		"sass": "1.54.8",
		"stylelint": "^14.11.0",
		"stylelint-config-standard-scss": "^5.0.0",
		"svg2ttf": "6.0.3",
		"svgicons2svgfont": "10.0.6",
		"svgo": "2.8.0",
		"ttf2woff": "3.0.0",
		"ttf2woff2": "4.0.5",
		"webpack": "5.74.0",
		"webpack-cli": "4.10.0",
		"webpack-stream": "7.0.0",
		"yargs": "17.5.1"
	},
	"engines": {
		"node": "16.13.0"
	}
}
