<div class="tabs-block">
	<div class="wrap wrap--inner">
		<div class="tabs-block__container">
			<ul class="tabs-block__list tab-list">
				{{#each item}}
				<li class="tabs-block__item tab-item">
					<button type="button" class="tabs-block__btn tab{{#if this.active}} tab--active{{/if}}">{{this.btn}}</button>
				</li>
				{{/each}}
			</ul>
			<div class="tabs-block__holder">
				{{#if title}}
				<h3 class="tabs-block__title h3">{{title}}</h3>
				{{/if}}
				{{#if cards}}
				<div class="tabs-block__holder-cards">
					{{#each cards}}

					{{/each}}
				</div>
				{{/if}}
				{{#if links}}
				<ul class="tabs-block__holder-links">
					{{#each links}}

					{{/each}}
				</ul>
				{{/if}}
			</div>
		</div>
	</div>
</div>
